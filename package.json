{
  "name": "parcel-build-glitch",
  "version": "0.0.1",
  "description": "demonstrate parcel continuous build glitch",
  "main": "./dist/index.html",
  "author": "Jonathan Kolyer",
  "license": "MIT",
  "private": true,
  "scripts": {
    "yarn:set:version": "yarn set version berry",
    "start": "parcel ./dist/index.html",
    "start:open": "parcel ./dist/index.html --open",
    "clean": "rimraf ./dist",
    "build:parcel": "parcel build ./src/index.html --dist-dir ./dist --public-url '/'   --no-source-maps",
    "build:clean": "run clean && run build:parcel"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/babel-types": "^7.0.9",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "babel-types": "^6.26.0",
    "debug": "^4.2.0",
    "jest": "^26.5.2",
    "jest-dom": "^4.0.0",
    "lodash": "^4.17.20",
    "parcel": "next",
    "purify-ts": "^0.16.0-beta.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
